<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<bean class="java.lang.String" id="String">
		<constructor-arg value="Xworkz"></constructor-arg>
	</bean>
	<bean class="java.lang.Integer" id="Integer">
		<constructor-arg value="123"></constructor-arg>
	</bean>
	<bean class="java.lang.Byte" id="Byte">
		<constructor-arg value="10"></constructor-arg>
	</bean>
	<bean class="java.lang.Short" id="Short">
		<constructor-arg value="12"></constructor-arg>
	</bean>
	<bean class="java.lang.Character" id="Character">
		<constructor-arg value="c"></constructor-arg>
	</bean>
	<bean class="java.lang.Float" id="Float">
		<constructor-arg value="123.45f"></constructor-arg>
	</bean>
	<bean class="java.lang.Double" id="Double">
		<constructor-arg value="123.99"></constructor-arg>
	</bean>
	<bean class="java.lang.Boolean" id="Boolean">
		<constructor-arg value="true"></constructor-arg>
	</bean>
	<bean class="java.lang.Long" id="Long">
		<constructor-arg value="12309876"></constructor-arg>
	</bean>
</beans>