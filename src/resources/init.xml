<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">



<bean class="com.account.init.Hotel" id="hotel" lazy-init="default">
<constructor-arg value="Kamat"/>

</bean>
<bean class="com.account.init.Hotel" id="type">
<property name="type" value="true"/>
</bean>

<bean class="com.account.init.Hotel" id="location">
<property name="location" value="vijayanagar"/>
</bean>

</beans>