<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">



<bean class="com.account.part.Laptop" id="laptop" lazy-init="true">
<constructor-arg value="Dell"></constructor-arg>
<constructor-arg value="4"></constructor-arg>
</bean>

<bean class="com.account.part.Laptop" id="another" lazy-init="true">
<constructor-arg value="hp"></constructor-arg>
<constructor-arg value="4"></constructor-arg>
</bean>




</beans>