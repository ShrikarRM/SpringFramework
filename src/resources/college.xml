<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="college" class="com.account.college.College" autowire="constructor">
		<constructor-arg value="BVV"></constructor-arg>
		<constructor-arg value="30"></constructor-arg>
		<property name="address" value="B G nagar"></property>
		<constructor-arg ref="stu"></constructor-arg>
	</bean>

	<bean id="stu" class="com.account.college.Student">
		<property name="id" value="S123"></property>
		<property name="name" value="Suresh"></property>
		<property name="rollNum" value="4"></property>
	</bean>
 
</beans>